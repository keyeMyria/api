[paths]
source = src

[run]
branch = True
source = src

[report]
show_missing = true
precision = 2
omit =
    asgi.py
    manage.py
    *migrations*
    *settings*.py

exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:
