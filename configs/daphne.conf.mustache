# -*- mode: conf -*-
[program:daphne-{{domain}}]
# command={{vebin}}/daphne pashinin.asgi:channel_layer --port 8080 -b 127.0.0.1
command={{vebin}}/daphne -u /var/tmp/daphne-{{domain}}.sock pashinin.asgi:channel_layer
directory={{repo}}/src
user={{www-data}}
autostart=true
autorestart=false
stderr_logfile=/var/log/supervisor/daphne-{{domain}}.err.log
stdout_logfile=/var/log/supervisor/daphne-{{domain}}.out.log
