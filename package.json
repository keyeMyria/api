  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "latest",
    "chokidar": "latest",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "global": "^4.3.2",
    "gulp": "latest",
    "gulp-babel": "^7.0.0",
    "gulp-buffer": "latest",
    "gulp-livereload": "latest",
    "gulp-sass": "latest",
    "gulp-shell": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-tap": "latest",
    "gulp-typescript": "latest",
    "gulp-uglify": "latest",
    "gulp-util": "latest",
    "standard": "^10.0.3",
    "typescript": "latest",
    "uglify-es": "^3.0.28",
    "vinyl-transform": "latest"
  },
  "main": "index.js",
  "author": "Sergey Pashinin <sergey@pashinin.com>",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js ./src",
    "lint2": "./node_modules/standard/bin/cmd.js",
    "test": "mocha test/",
    "build": "babel src -d src"
  },
  "standard": {
    "ignore": [
      "**/*.mini.js",
      "/lib/select2/",
      "/lib/ckeditor/",
      "tmp.js"
    ]
  },
  "babel": {
    "comments": false,
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 3 Chrome versions",
              "last 3 Firefox versions",
              "last 2 ChromeAndroid versions",
              "last 2 FirefoxAndroid versions",
              "last 2 iOS versions"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "write": "^1.0.3"
  }
}
