{% block prebody %}{% endblock %}

<div class="flex01a flex 0 col" style="background:#eee;">
{% if self.header() %}
  <header style="border-bottom: 1px solid rgba(0,0,0,0.075);z-index:100;">{% block header %}{% endblock %}</header>
{% endif %}
{% block beforemain %}{% endblock %}
<main class="flex mart marb">{% block main %}{% endblock %}</main>
{% block aftermain %}{% endblock %}
</div>

<footer class="padtb" style="background:#242729;color:#aaa">{% block footer %}
<div class="w noprint pad-sm flex equal">
  {% block footer_content %}
	<div>
	  <a href="{{url("index")}}">{{DOMAIN}}</a> <br/>
	  <a href="{{url("kafedra", host="baumanka", kwargs={'F':'IU', 'K':2})}}">Студентам ИУ2 МГТУ им. Баумана</a>
	  <br/>
